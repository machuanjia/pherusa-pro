{
  "name": "pherusa-pro",
  "version": "2.0.5",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.16.3",
    "axios": "0.21.1",
    "history": "5.0.0",
    "hoist-non-react-statics": "3.3.2",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "path-to-regexp": "6.2.0",
    "prop-types": "15.7.2",
    "react": "^16.12.0",
    "react-color": "^2.19.3",
    "react-is": "17.0.2",
    "react-router-dom": "^4.3.1",
    "react-dom": "17.0.2",
    "react-grid-layout": "1.2.5",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.4",
    "redux-thunk": "2.3.0",
    "i18next": "20.3.1",
    "i18next-browser-languagedetector": "6.1.1",
    "i18next-http-backend": "1.2.6",
    "react-i18next": "11.11.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
